{
  "name": "use-formio",
  "version": "1.0.5",
  "repository": "https://github.com/Svehla/use-formio",
  "license": "MIT",
  "author": "Jakub Å vehla",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "module": "dist/use-formio.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "ts:check-types": "tsc",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --passWithNoTests --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reactstrap": "^9.0.1",
    "size-limit": "^5.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=10"
  },
  "// prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "size-limit": [
    {
      "path": "dist/use-formio.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-formio.esm.js",
      "limit": "10 KB"
    }
  ]
}